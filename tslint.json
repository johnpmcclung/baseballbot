{
    "rules": {
        "curly": true,
        "class-name": true,
        "no-unused-expression": true,
        "no-unused-variable": true,
        "no-duplicate-variable": true,
        "no-trailing-whitespace": true,
        "no-consecutive-blank-lines": [ true ],
        "semicolon": ["always"],
        "triple-equals": true,
        "ordered-imports": [ true ],
        "no-var-keyword": true,
        "object-literal-key-quotes": [ true ]
    }
}